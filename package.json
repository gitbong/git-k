{
  "name": "gitk",
  "version": "1.0.1",
  "description": "Mock server for local development",
  "main": "dist/server/GitK.js",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"yarn server\" \"yarn client\" ",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot --port 3000",
    "server": "nodemon --exec babel-node src/server/index.js",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "babel src/server --out-dir dist/server",
    "build": "npm run build:client && npm run build:server",
    "lint": "eslint  \"./src/**/*.{jsx,js,json}\"  --fix",
    "format": "prettier --write \"./src/**/*.{jsx,js,json}\" "
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "html-webpack-plugin": "^4.3.0",
    "prettier": "^2.0.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.1.1"
  },
  "keywords": [
    "JSON",
    "server",
    "fake",
    "API",
    "prototyping",
    "mock",
    "mocking",
    "test",
    "testing",
    "rest",
    "data",
    "dummy"
  ],
  "author": "Gitbong <gitbongs@gmail.com>"
}
